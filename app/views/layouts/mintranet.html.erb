<!DOCTYPE html>
<html>
<%# if request.subdomain == "mintranet" %>
	<head>
		<!-- <link href="css/ticker-style.css" rel="stylesheet" type="text/css" /> -->
		<!-- <script src="jquery.ticker.js" type="text/javascript"></script> -->
		<!-- <script type="text/javascript" src="https://www.google.com/jsapi"></script> -->
		  <title>Mintranet</title>
	  	<%= stylesheet_link_tag    "application", :media => "all" %>	  	
	  	<%#= stylesheet_link_tag "bootstrap_and_overrides.css.less" %>	  	
	  	<%= javascript_include_tag "application" %>	  	
	  	<%= csrf_meta_tags %>
	</head>
	<body data-spy="scroll" data-offset="200" data-target="#tabs">
		<%= render "shared/nav" %>
		<div class="container">
			<div class="row">
				<div class="span12">
				<% if notice %>
					<div class="alert alert-success">
						<button type="button" class="close" data-dismiss="alert">&times;</button>
						<%=raw notice %>
					</div>
				<% end %>
				<% if alert %>
					<div class="alert alert-error">
						<button type="button" class="close" data-dismiss="alert">&times;</button>
						<%= alert %>
					</div>
				<% end %>
				<%= yield %>
				</div>
			</div>
		</div>
	</body>
	</br>
	</br>
	</br>
</html>